{
  "name": "escapin",
  "version": "0.2.10",
  "description": "Escapin the Transpiler for Escaping from Complicated Usage of Cloud Services and APIs",
  "main": "bin/index.js",
  "type": "module",
  "engines": {
    "node": ">=10.0.0"
  },
  "bin": {
    "escapin": "bin/cli.js"
  },
  "files": ["/bin", "/templates"],
  "scripts": {
    "prebuild": "rimraf bin",
    "build": "tsc",
    "postbuild": "ncp bin/src bin && rimraf bin/src bin/package.json",
    "lint": "eslint --ignore-path .gitignore --cache .",
    "lint-updated": "pipe-git-updated --ext=ts --ext-js -- eslint --ignore-path .gitignore --cache",
    "format": "prettier --write --ignore-path .gitignore **/*.{ts,js,json,yaml,yml,md}",
    "prettier-check": "prettier -c --ignore-path .gitignore **/*.{ts,js,json,yaml,yml,md}",
    "prettier-check-updated": "pipe-git-updated --ext=ts --ext=js --ext=json --ext=yaml --ext=yml --ext=md -- prettier -c",
    "test": "jest",
    "coverage": "jest --coverage",
    "start": "ts-node src/cli.ts"
  },
  "repository": {
    "url": "https://github.com/FujitsuLaboratories/escapin.git"
  },
  "keywords": [
    "serverless",
    "serverless framework",
    "typescript",
    "babel",
    "transpiler"
  ],
  "author": "Kimura, Kosaku <kimura.kosaku@fujitsu.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/code-frame": "^7.5.5",
    "@babel/generator": "^7.7.2",
    "@babel/parser": "^7.7.3",
    "@babel/template": "^7.7.0",
    "@babel/traverse": "^7.7.2",
    "@babel/types": "^7.7.2",
    "boxen": "^4.1.0",
    "chalk": "^2.4.2",
    "commander": "^4.0.0",
    "cosmiconfig": "^5.2.1",
    "deasync": "^0.1.15",
    "execa": "^3.2.0",
    "https-proxy-agent": "^3.0.1",
    "ignore": "^5.1.4",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.15",
    "mkdirp": "^0.5.1",
    "openapi-types": "^1.3.5",
    "package-json": "^6.5.0",
    "request": "^2.88.0",
    "rimraf": "^3.0.0",
    "swagger-parser": "^8.0.3",
    "tslib": "^1.10.0",
    "types-installer": "^1.6.3",
    "typescript": "^3.7.2",
    "uuid": "^3.3.3",
    "validator": "^12.0.0"
  },
  "devDependencies": {
    "@types/babel__code-frame": "^7.0.1",
    "@types/babel__generator": "^7.6.0",
    "@types/babel__template": "^7.0.2",
    "@types/babel__traverse": "^7.0.7",
    "@types/commander": "^2.12.2",
    "@types/cosmiconfig": "^5.0.3",
    "@types/deasync": "^0.1.0",
    "@types/eslint": "^6.1.3",
    "@types/eslint-plugin-prettier": "^2.2.0",
    "@types/execa": "^2.0.0",
    "@types/jest": "^24.0.22",
    "@types/js-yaml": "^3.12.1",
    "@types/lodash": "^4.14.146",
    "@types/mkdirp": "^0.5.2",
    "@types/mocha": "^5.2.7",
    "@types/mustache": "^0.8.32",
    "@types/ncp": "^2.0.3",
    "@types/node": "^12.12.7",
    "@types/package-json": "^5.0.1",
    "@types/prettier": "^1.18.3",
    "@types/request": "^2.48.3",
    "@types/rimraf": "^2.0.3",
    "@types/serverless": "^1.18.4",
    "@types/swagger-parser": "^7.0.1",
    "@types/typescript": "^2.0.0",
    "@types/uuid": "^3.4.6",
    "@types/validator": "^10.11.3",
    "@typescript-eslint/eslint-plugin": "^2.6.1",
    "@typescript-eslint/parser": "^2.6.1",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "git-list-updated": "^1.2.1",
    "jest": "^24.9.0",
    "ncp": "^2.0.0",
    "prettier": "^1.19.0",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1"
  }
}

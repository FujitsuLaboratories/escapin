{
  "name": "escapin",
  "version": "0.2.21",
  "description": "Escapin the Transpiler for Escaping from Complicated Usage of Cloud Services and APIs",
  "main": "bin/index.js",
  "engines": {
    "node": ">=10.0.0"
  },
  "bin": {
    "escapin": "bin/cli.js"
  },
  "files": ["/bin", "/templates"],
  "scripts": {
    "prebuild": "rimraf bin",
    "build": "tsc",
    "postbuild": "ncp bin/src bin && rimraf bin/src bin/package.json",
    "lint": "eslint --ignore-path .gitignore --cache .",
    "lint-updated": "pipe-git-updated --ext=ts --ext-js -- eslint --ignore-path .gitignore --cache",
    "format": "prettier --write --ignore-path .gitignore *.{ts,js,json,yaml,yml,md} **/*.{ts,js,json,yaml,yml,md}",
    "prettier-check": "prettier -c --ignore-path .gitignore *.{ts,js,json,yaml,yml,md} **/*.{ts,js,json,yaml,yml,md}",
    "prettier-check-updated": "pipe-git-updated --ext=ts --ext=js --ext=json --ext=yaml --ext=yml --ext=md -- prettier -c",
    "test": "jest",
    "coverage": "jest --coverage",
    "start": "ts-node src/cli.ts"
  },
  "repository": {
    "url": "https://github.com/FujitsuLaboratories/escapin.git"
  },
  "keywords": [
    "serverless",
    "serverless framework",
    "typescript",
    "babel",
    "transpiler"
  ],
  "author": "Kimura, Kosaku <kimura.kosaku@fujitsu.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/code-frame": "^7.8.3",
    "@babel/generator": "^7.8.3",
    "@babel/parser": "^7.8.3",
    "@babel/template": "^7.8.3",
    "@babel/traverse": "^7.8.3",
    "@babel/types": "^7.8.3",
    "boxen": "^4.2.0",
    "chalk": "^3.0.0",
    "commander": "^4.1.0",
    "cosmiconfig": "^6.0.0",
    "deasync": "^0.1.19",
    "execa": "^4.0.0",
    "https-proxy-agent": "^4.0.0",
    "ignore": "^5.1.4",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.15",
    "mkdirp": "^0.5.1",
    "node-fetch": "^2.6.0",
    "openapi-types": "^1.3.5",
    "package-json": "^6.5.0",
    "rimraf": "^3.0.0",
    "swagger-parser": "^8.0.4",
    "tslib": "^1.10.0",
    "types-installer": "^1.6.3",
    "typescript": "^3.7.5",
    "uuid": "^3.4.0",
    "validator": "^12.1.0"
  },
  "devDependencies": {
    "@types/babel__code-frame": "^7.0.1",
    "@types/babel__generator": "^7.6.1",
    "@types/babel__template": "^7.0.2",
    "@types/babel__traverse": "^7.0.8",
    "@types/deasync": "^0.1.0",
    "@types/eslint": "^6.1.4",
    "@types/eslint-plugin-prettier": "^2.2.0",
    "@types/jest": "^24.9.0",
    "@types/js-yaml": "^3.12.1",
    "@types/lodash": "^4.14.149",
    "@types/mkdirp": "^0.5.2",
    "@types/ncp": "^2.0.3",
    "@types/node": "^13.1.8",
    "@types/node-fetch": "^2.5.4",
    "@types/prettier": "^1.19.0",
    "@types/rimraf": "^2.0.3",
    "@types/uuid": "^3.4.6",
    "@types/validator": "^12.0.1",
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "@typescript-eslint/parser": "^2.17.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "git-list-updated": "^1.2.1",
    "jest": "^25.1.0",
    "ncp": "^2.0.0",
    "prettier": "^1.19.1",
    "ts-jest": "^24.3.0",
    "ts-node": "^8.6.2"
  }
}

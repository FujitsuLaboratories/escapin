{
  "name": "escapin",
  "version": "0.2.28",
  "description": "Escapin the Transpiler for Escaping from Complicated Usage of Cloud Services and APIs",
  "main": "bin/index.js",
  "engines": {
    "node": ">=10.0.0"
  },
  "bin": {
    "escapin": "bin/cli.js"
  },
  "files": [
    "/bin",
    "/templates"
  ],
  "scripts": {
    "prebuild": "rimraf bin",
    "build": "tsc",
    "postbuild": "ncp bin/src bin && rimraf bin/src bin/package.json",
    "format": "prettier --write --ignore-path .gitignore \"**/*.{ts,js,json,yaml,yml,md}\"",
    "test": "run-p test:*",
    "test:jest": "jest --coverage",
    "test:lint": "eslint --ignore-path .gitignore --cache .",
    "test:prettier-check": "prettier -c --ignore-path .gitignore \"**/*.{ts,js,json,yaml,yml,md}\"",
    "test:tsc": "tsc --noEmit",
    "start": "ts-node src/cli.ts"
  },
  "repository": {
    "url": "https://github.com/FujitsuLaboratories/escapin.git"
  },
  "keywords": [
    "serverless",
    "serverless framework",
    "typescript",
    "babel",
    "transpiler"
  ],
  "author": "Kimura, Kosaku <kimura.kosaku@fujitsu.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/code-frame": "^7.8.3",
    "@babel/generator": "^7.8.7",
    "@babel/parser": "^7.8.7",
    "@babel/template": "^7.8.6",
    "@babel/traverse": "^7.8.6",
    "@babel/types": "^7.8.7",
    "boxen": "^4.2.0",
    "chalk": "^3.0.0",
    "commander": "^4.1.1",
    "cosmiconfig": "^6.0.0",
    "deasync": "^0.1.19",
    "execa": "^4.0.0",
    "https-proxy-agent": "^5.0.0",
    "ignore": "^5.1.4",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.15",
    "mkdirp": "^1.0.3",
    "node-fetch": "^2.6.0",
    "openapi-types": "^1.3.5",
    "package-json": "^6.5.0",
    "rimraf": "^3.0.2",
    "swagger-parser": "^8.0.4",
    "tslib": "^1.11.1",
    "types-installer": "^1.6.3",
    "typescript": "^3.8.3",
    "uuid": "^7.0.2",
    "validator": "^12.2.0"
  },
  "devDependencies": {
    "@types/babel__code-frame": "^7.0.1",
    "@types/babel__generator": "^7.6.1",
    "@types/babel__template": "^7.0.2",
    "@types/babel__traverse": "^7.0.9",
    "@types/deasync": "^0.1.0",
    "@types/eslint": "^6.1.8",
    "@types/eslint-plugin-prettier": "^2.2.0",
    "@types/jest": "^25.1.4",
    "@types/js-yaml": "^3.12.2",
    "@types/lodash": "^4.14.149",
    "@types/mkdirp": "^1.0.0",
    "@types/ncp": "^2.0.3",
    "@types/node": "^13.9.0",
    "@types/node-fetch": "^2.5.5",
    "@types/prettier": "^1.19.0",
    "@types/rimraf": "^2.0.3",
    "@types/uuid": "^7.0.0",
    "@types/validator": "^12.0.1",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "git-list-updated": "^1.2.1",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.8",
    "ncp": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.6.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "git update-index --again"
    }
  }
}
